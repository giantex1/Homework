package preintermediate;

public class Pre8 {
	public static void main(String[] args) {
		double a=0;
		double b=1;
		double c=0;
		double total=0;
		System.out.println(a);
		System.out.println(b);
		for (int i=0;i<102;i++)
		{
			c=a+b;
			if((c % 2)!=0)
			{
				total=total+c;
			}
			a=b;
			b=c;
			System.out.println(c);
		}
		System.out.println("Total:" + (total+1));
	}
}
