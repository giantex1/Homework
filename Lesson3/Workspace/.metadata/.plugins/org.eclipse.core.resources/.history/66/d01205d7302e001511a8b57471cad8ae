package beginner;
import java.util.Scanner;

public class B3 {
	public static void main(String[] args) {
		boolean res;
		Scanner in = new Scanner(System.in);
		System.out.println("Non-negative number: ");
		int x = in.nextInt();
		if (x%3 == 0 || x%5 == 0)
		{
			res= true;
			System.out.println(res);
		}		
}
}
