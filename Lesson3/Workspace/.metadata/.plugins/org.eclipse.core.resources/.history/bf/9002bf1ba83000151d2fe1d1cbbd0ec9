package advanced;
import java.util.Random;
public class Adv1 {
	public static void main(String[] args) 
	{
		Random rand = new Random();
		 
        for (int i = 0; i < 10; i++) {
            System.out.println(
                String.format(
                    "%b\t %d\t %d\t %d\t %.10f\t %.10f",
                    rand.nextBoolean(),
                    rand.nextInt(),
                    rand.nextInt(20),
                    rand.nextLong(),
                    rand.nextFloat(),
                    rand.nextDouble()
                )
            );
        }
	}
}
