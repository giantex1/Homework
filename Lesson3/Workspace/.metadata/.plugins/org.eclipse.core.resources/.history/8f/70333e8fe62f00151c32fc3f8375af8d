package preintermediate;

public class Pre8 {
	public static void main(String[] args) {
		int a=0;
		int b=1;
		int c=0;
		int total=0;
		System.out.println(a);
		System.out.println(b);
		for (int i=0;i<102;i++)
		{
			c=a+b;
			if((c & 1)==1)
			{
				total=total+c;
			}
			a=b;
			b=c;
			System.out.println(c);
		}
		System.out.println("Total:" + (total+1));
	}
}
